\f0\fs24 \cf0 \uc0\u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  1. \u49884 \u51089 \u51008  \u51312 \u50857 \u54664 \u50612 \u50836  \u8203  30\u45824  \u52488 \u48152  \u54532 \u47532 \u47004 \u49436 \u51060 \u44256 , 1\u51064 \u44032 \u44396 \u47196  \u49332 \u50500 \u44032 \u44256  \u51080 \u50612 \u50836 . \u45734 \u51104 \u51012  \u51088 \u51452  \u51088 \u45716  \u54200 \u51060 \u46972  \u50500 \u52840 \u50644  \u45720  \u51221 \u49888 \u51060  \u47805 \u47217 \u54664 \u45716 \u45936 \u50836 , \u50836 \u51608 \u52376 \u47100  \u45908 \u50868  \u50668 \u47492 \u52384 \u50644  \u53945 \u55176  \u45908  \u51665 \u51473 \u51060  \u50504  \u46104 \u45908 \u46972 \u44256 \u50836 . \u52964 \u54588 \u47196  \u48260 \u53608 \u48372 \u47140  \u54664 \u51648 \u47564  \u54616 \u47336 \u50640  3\u51092  \u51060 \u49345  \u47560 \u49884 \u44256  \u45208 \u45768  \u49549 \u46020  \u48520 \u54200 \u54616 \u44256 , \u51060 \u44148  \u50500 \u45768 \u45796  \u49910 \u50632 \u50612 \u50836 . \u44536 \u47084 \u45796  \u50864 \u50672 \u55176  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284 \u44032  \u51339 \u45796 \u45716  \u50584 \u44592 \u47484  \u46307 \u44256  \u51312 \u49900 \u49828 \u47101 \u44172  \u49884 \u51089 \u54616 \u44172  \u46096 \u50612 \u50836 . \u8203  \u52376 \u51020 \u50644  1\u51068  1\u54924 , 400mg\u50473  \u50500 \u52840  \u49885 \u49324  \u54980 \u50640  \u48373 \u50857 \u54664 \u50612 \u50836 . \u49324 \u49892  \u51221 \u47568  \u51060 \u44172  \u54952 \u44284  \u51080 \u51012 \u44620 ? \u49910 \u51008  \u47560 \u51020 \u51060  \u52984 \u50612 \u50836 . \u44305 \u44256 \u45716  \u45720  \u44284 \u51109 \u51060 \u45768 \u44620 \u50836 . \u44536 \u47088 \u45936  4\u51068 \u52264 \u52196 \u48512 \u53552 \u50688 \u45912  \u44163  \u44057 \u50500 \u50836 . \u54924 \u51032  \u51456 \u48708 \u54616 \u45796 \u44032  \u47928 \u46301 , \u51665 \u51473  \u49884 \u44036 \u51060  \u50696 \u51204 \u48372 \u45796  \u44600 \u50612 \u51652  \u44152  \u45712 \u44808 \u50612 \u50836 . \u50696 \u51204 \u50644  20\u48516 \u47564  \u54644 \u46020  \u49328 \u47564 \u54664 \u45716 \u45936 , 40\u48516 \u51008  \u54988 \u51789  \u45336 \u44220 \u45908 \u46972 \u44256 \u50836 . \u51060 \u44172  \u51652 \u51676 \u51064 \u44032  \u49910 \u50632 \u50612 \u50836 . \u8203  \u51109 \u47560 \u52384 \u51060 \u46972  \u54616 \u47336 \u51333 \u51068  \u55120 \u47532 \u44256  \u52629 \u52629 \u54620  \u45216 \u51060 \u50632 \u45716 \u45936 \u46020  \u47693 \u54620  \u45712 \u45196 \u51060  \u45916 \u54664 \u50612 \u50836 . \u47560 \u52824  \u47672 \u47551 \u49549 \u50640  \u47676 \u51648 \u44032  \u51328  \u44151 \u55180  \u44163  \u44057 \u45796 \u44256  \u54624 \u44620 \u50836 . \u50500 \u51649  \u54869 \u49888 \u44620 \u51652  \u50500 \u45768 \u50632 \u51648 \u47564 , \u47956 \u44032  \u45804 \u46972 \u51648 \u44256  \u51080 \u45796 \u45716  \u44148  \u45712 \u44788 \u51276 \u50612 \u50836 . \u8203  2. \u48320 \u54868 \u45716  \u51032 \u50808 \u51032  \u49692 \u44036 \u50640  \u8203  \u8203  \u8203  \u44536  \u49692 \u44036  \u45712 \u44808 \u45912  \u44048 \u51221 \u51008  \u52280  \u47896 \u54664 \u50612 \u50836 . \'93\u47956 \u44032  \u48148 \u45072  \u44163  \u44057 \u51008 \u45936 ...\'94 \u49910 \u51004 \u47732 \u49436 \u46020  \u54841 \u49884  \u50864 \u50672 \u51008  \u50500 \u45776 \u44620  \u54616 \u45716  \u51032 \u49900 \u46020  \u51080 \u50632 \u51424 . \u54616 \u51648 \u47564  \u46028 \u50500 \u48372 \u45768  \u51060 \u51204  2\u51452 \u44036  \u48164 \u47560 \u45796  6\u49884 \u44036  \u51221 \u46020  \u50504 \u51221 \u51201 \u51064  \u49688 \u47732 \u51012  \u52712 \u54664 \u45912  \u44163 \u46020  \u50689 \u54693 \u51060  \u51080 \u50632 \u45912  \u44163  \u44057 \u50500 \u50836 . \u50896 \u47000  \u48520 \u47732 \u51613 \u44620 \u51648 \u45716  \u50500 \u45768 \u51648 \u47564 , \u51088 \u51452  \u44648 \u45716  \u54200 \u51060 \u50632 \u44144 \u46304 \u50836 . 3. \u51221 \u47532 \u54644 \u48372 \u45716  \u48373 \u50857  3\u51452 \u52264  \u8203  \u48373 \u50857  21\u51068 \u52264 \u50640  \u51217 \u50612 \u46308  \u47924 \u47157 , \u52404 \u44048  \u48320 \u54868 \u44032  \u51328  \u45908  \u46748 \u47159 \u54644 \u51276 \u50612 \u50836 . \u51068 \u45800  \u50500 \u52840  \u44592 \u49345 \u51060  \u55016 \u50476  \u49772 \u50892 \u51276 \u50612 \u50836 . \u50696 \u51204 \u50644  \u44592 \u49345  \u54980  30\u48516  \u46041 \u50504 \u51008  \u47693 \u54616 \u45768  \u50505 \u50500  \u51080 \u50632 \u45716 \u45936 , \u52572 \u44540 \u50644  \u45576  \u46888 \u44256  10\u48516  \u51060 \u45236 \u47196  \u50880 \u51649 \u50688 \u50612 \u50836 . \u8203  \u44536 \u47532 \u44256  \u51089 \u50629  \u51473  \u51665 \u51473  \u49884 \u44036 \u51060  \u54217 \u44512  45\u48516  \u51060 \u49345  \u50976 \u51648 \u46104 \u45716  \u45216 \u51060  \u45720 \u50632 \u50612 \u50836 . \u51060 \u51204 \u50640 \u45716  20\\~25\u48516  \u51221 \u46020 \u44032  \u54620 \u44228 \u50688 \u45716 \u45936 \u50836 . \u44228 \u49328 \u54644 \u48372 \u47732  \u44144 \u51032  1.8\u48176  \u44032 \u44620 \u51060  \u45720 \u50612 \u45212  \u44144 \u50696 \u50836 . \u44536 \u47532 \u44256  \u45576 \u51032  \u54588 \u47196 \u44048 \u46020  23% \u51221 \u46020  \u51460 \u50612 \u46304  \u44163  \u44057 \u50520 \u50612 \u50836 . \u52980 \u54504 \u53552  \u50526 \u50640  \u50505 \u50500  \u51080 \u45716  \u49884 \u44036 \u51060  \u54616 \u47336  \u54217 \u44512  7\u49884 \u44036 \u51064 \u45936 , \u50696 \u51204 \u52376 \u47100  \u45576 \u51060  \u48849 \u48849 \u54616 \u44144 \u45208  \u46384 \u44049 \u51648  \u50506 \u50520 \u50612 \u50836 . \u8203  \u12593 . \u54924 \u51032  \u51473 \u50640 \u46020  \u47568 \u49892 \u49688 \u44032  37% \u51221 \u46020  \u51460 \u50632 \u44256  \u12596 . \u52852 \u54168  \u51089 \u50629  \u49884  \u51665 \u51473 \u47141  \u50976 \u51648 \u49884 \u44036 \u51060  2.1\u48176  \u45720 \u50632 \u44256  \u12599 . \u49688 \u47732  \u49884 \u44036 \u51008  6\u49884 \u44036  30\u48516 \u51004 \u47196  \u54217 \u44512  42\u48516  \u51613 \u44032 \u54664 \u44256  \u12601 . \u50724 \u54980  \u51320 \u47548  \u48712 \u46020 \u45716  5\u51068  \u50672 \u49549  \u44144 \u51032  \u49324 \u46972 \u51276 \u44256  \u12609 . \u47560 \u51020 \u51032  \u44596 \u51109 \u44048 \u46020  \u54869 \u49892 \u55176  \u50756 \u54868 \u46096 \u50612 \u50836 . \u8203  \u51060  \u47784 \u46304  \u44172  \u50756 \u48317 \u54616 \u44172  \u51228 \u54408  \u45909 \u48516 \u51060 \u46972 \u44256  \u45800 \u51221  \u51648 \u51012  \u49692  \u50630 \u44192 \u51648 \u47564 , \u45516 \u50689 \u50577 \u51228  \u48373 \u50857  \u54980  \u51068 \u50612 \u45212  \u48320 \u54868 \u46972 \u45716  \u51216 \u51008  \u48512 \u51064 \u54624  \u49688  \u50630 \u50632 \u50612 \u50836 . 4. \u49332 \u51677  \u48520 \u54200 \u54664 \u45912  \u51216 \u46020  \u51080 \u50632 \u50612 \u50836  \u8203  \u49556 \u51649 \u55176  \u48373 \u50857  \u52488 \u48152 \u50644  \u49332 \u51677  \u48520 \u50504 \u54664 \u50612 \u50836 . \u52376 \u51020  3\u51068  \u51221 \u46020 \u45716  \u49549 \u51060  \u45908 \u48512 \u47337 \u54620  \u45712 \u45196 \u51060  \u51080 \u50632 \u44144 \u46304 \u50836 . \u49885 \u49324  \u54980  \u48148 \u47196  \u47673 \u50612 \u49436  \u44536 \u47088 \u44032  \u49910 \u50612  \u45796 \u51020 \u48512 \u53556  30\u48516  \u54980 \u50640  \u48373 \u50857 \u54644 \u48420 \u45908 \u45768  \u44316 \u52270 \u50500 \u51276 \u50612 \u50836 . \u8203  \u44536 \u47532 \u44256  \u45252 \u49352 \u44032  \u51008 \u44540 \u55176  \u44053 \u54664 \u50612 \u50836 . \u50508 \u50557 \u51064 \u45936 \u46020  \u53945 \u50976 \u51032  \u54693 \u51060  \u51080 \u50612 \u49436  \u52376 \u51020 \u50644  \u49340 \u53412 \u44592  \u49332 \u51677  \u48512 \u45812 \u49828 \u47084 \u50912 \u50612 \u50836 . \u47932 \u47200  5\u51068  \u51221 \u46020  \u51648 \u45208 \u45768  \u51061 \u49689 \u54644 \u51648 \u44596  \u54664 \u51648 \u47564 \u50836 . \u8203  \u44032 \u45140 \u51008  \u51221 \u47568  \u45236 \u44032  \u51060 \u44152  \u47673 \u50612 \u49436  \u51339 \u50500 \u51652  \u44148 \u44032 ? \u49910 \u51008  \u49373 \u44033 \u46020  \u46308 \u50632 \u50612 \u50836 . \u49324 \u46988  \u47560 \u51020 \u51060 \u46976  \u44172  \u52280  \u44036 \u49324 \u54616 \u51424 . \u48516 \u47749  \u51339 \u50500 \u51652  \u44152  \u45712 \u45180 \u47732 \u49436 \u46020  \u54620 \u54200 \u51004 \u47196 \u45716  \u51032 \u49900 \u51012  \u54408 \u44172  \u46104 \u45768 \u44620 \u50836 . \u44536 \u47000 \u46020  \u54588 \u47196 \u46020 \u44032  18% \u51221 \u46020  \u51460 \u50632 \u45796 \u45716  \u44148  \u54869 \u49892 \u55176  \u52404 \u44048 \u54664 \u50612 \u50836 . \u53748 \u44540  \u54980  \u51665 \u50640  \u46308 \u50612 \u50732  \u46412  \u48156 \u44152 \u51020 \u51060  \u54869 \u49892 \u55176  \u45916  \u47924 \u44144 \u50912 \u50612 \u50836 . \u44536 \u44172  \u51652 \u51676  \u49888 \u44592 \u54664 \u50612 \u50836 . 5. \u50500 \u51649 \u51008  \u49892 \u54744  \u51473 \u51060 \u50640 \u50836  \u8203  4\u51452 \u52264 \u50640  \u46308 \u50612 \u49436 \u47732 \u49436 \u48512 \u53552 \u45716  \u48373 \u50857  \u47336 \u54004 \u46020  \u51228 \u48277  \u51088 \u50672 \u49828 \u47084 \u50892 \u51276 \u50612 \u50836 . \u50500 \u52840  8\u49884 \u50640  \u44592 \u49345  \u8594  \u49885 \u49324  \u54980  8\u49884  30\u48516 \u50640  400mg \u48373 \u50857  \u8594  \u50724 \u51204  9\u49884 \u48512 \u53552  \u51089 \u50629  \u49884 \u51089 , \u51060 \u47088  \u49885 \u51060 \u50632 \u50612 \u50836 . \u8203  \u49885 \u49845 \u44288 \u46020  \u51312 \u44552 \u50473  \u48148 \u45068 \u50632 \u50612 \u50836 . \u50696 \u51204 \u50644  \u50556 \u49885 \u51004 \u47196  \u53888 \u44608 \u51060 \u45208  \u44284 \u51088 \u47448 \u47484  \u51088 \u51452  \u47673 \u50632 \u45716 \u45936 , \u50836 \u51608 \u51008  \u50724 \u55176 \u47140  \u47932 \u51012  \u45908  \u51088 \u51452  \u47560 \u49884 \u44172  \u46096 \u44256 \u50836 . \u54616 \u47336  \u54217 \u44512  \u49453 \u52712 \u47049 \u51060  1.2L\u50640 \u49436  1.8L\u47196  \u45720 \u50632 \u45908 \u46972 \u44256 \u50836 . \u51060 \u44163 \u46020  \u51032 \u50808 \u51032  \u48320 \u54868 \u50688 \u50612 \u50836 . \u8203  \u50612 \u46523 \u44172  \u48372 \u47732  \u51060 \u44148  \u45800 \u49692 \u54620  \u50689 \u50577 \u51228  \u51060 \u49345 \u51032  \u49845 \u44288  \u44368 \u51221 \u51060 \u50632 \u45716 \u51648 \u46020  \u47784 \u47476 \u44192 \u50612 \u50836 . \u47560 \u52824  \u49828 \u54144 \u51648 \u44032  \u47932 \u51012  \u55137 \u49688 \u54616 \u46319 , \u47800 \u44284  \u47560 \u51020 \u51060  \u51312 \u44552 \u50473  \u45804 \u46972 \u51648 \u44256  \u51080 \u50632 \u50612 \u50836 . \u45516 \u44032  \u45916  \u54588 \u44260 \u54616 \u45768 \u44620  \u51068 \u49345 \u46020  \u45916  \u51648 \u52824 \u45908 \u46972 \u44256 \u50836 . \u50836 \u51608 \u51008  \u51104 \u44624 \u51032  \u49328 \u52293 \u47564 \u51004 \u47196 \u46020  \u44592 \u48516  \u51204 \u54872 \u51060  \u46104 \u45716  \u44152  \u45712 \u44808 \u50612 \u50836 . \u8203  \u44536 \u47000 \u49436  \u50526 \u51004 \u47196 \u46020  \u44228 \u49549  \u48373 \u50857 \u54624 \u51648  \u50500 \u51649 \u51008  \u44256 \u48124  \u51473 \u51060 \u50640 \u50836 . \u50756 \u51204 \u55176  \u54869 \u49888 \u54624  \u49688 \u45716  \u50630 \u51648 \u47564 , \u48516 \u47749 \u55176  \u47956 \u44032  \u45804 \u46972 \u51276 \u45796 \u45716  \u49373 \u44033 \u51060  \u51088 \u44984  \u46308 \u50612 \u49436 \u50836 . \u51200 \u52376 \u47100  \u54841 \u49884  \u45720  \u47693 \u54620  \u45712 \u45196  \u46412 \u47928 \u50640  \u44256 \u48124 \u51060 \u49512 \u45912  \u48516 \u46308 \u51060 \u46972 \u47732 , \u54620  \u48264 \u52196  \u49884 \u46020 \u54644 \u48372 \u45716  \u44163 \u46020  \u45208 \u49240 \u51652  \u50506 \u51012  \u44163  \u44057 \u50500 \u50836 . 1-60 \u8203  \u8203  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u45516 \u50689 \u50577 \u51228  \u54952 \u44284  \u51665 \u51473 \u47141  \u54693 \u49345  \u46020 \u50880  \u8203  \u8203  \u8203  \u8203  \u8203  \u8203  \u8203 }